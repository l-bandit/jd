<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.jd.model.mapper.Sort2Mapper">

	<select id="selectSecondaryClassification" parameterType="int" resultType="SecondaryClassification">
		select *
		from primaryclassification p, secondaryclassification s
		where p.id = s.fid
		and s.fid = #{sort1id};
	</select>

</mapper>