<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.jd.model.mapper.SizeMapper">

	<select id="selectSizeByCid" parameterType="int" resultType="size">
		select *
		from size
		where cid=#{cid}
	</select>

</mapper>